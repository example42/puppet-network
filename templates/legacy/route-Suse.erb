<%- (0..(@ipaddress.length-1)).each do |id| -%>
<%= @ipaddress[id] %><%- if @gateway and @gateway[id] -%> <%= @gateway[id] %><%- else -%> -<%- end -%> <%= @netmask[id] %> <%= @interface %><%- if @scope and @scope[id] -%> scope <%= @scope[id] %><%- end -%><%- if @source and @source[id] -%> src <%= @source[id] %><%- end -%><%- if @table and @table[id] -%> table <%= @table[id] %><% end %><%- if @metric and @metric[id] -%> metric <%= @metric[id] %><% end %><%- if @mtu and @mtu[id] -%> mtu <%= @mtu[id] %><% end %>
<%- end %>
