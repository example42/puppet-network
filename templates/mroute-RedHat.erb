<% @ipv4_routes.each do |net,gw| -%>
<%= net -%>
<%
  if gw.kind_of?(Array)
    gw.each do | g | %> nexthop via <%= g %><% end -%>
  <%- elsif /^\d/.match(gw) %> via <%= gw -%>
  <%- else %> dev <%= gw -%>
  <%- end -%>
<%- if @table %> table <%= @table %><% end %>
<% end -%>
